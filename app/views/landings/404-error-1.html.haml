!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %meta{:charset => "utf-8"}/
    %title Mazel - Multiprapose Html template
    %meta{:content => "", :name => "description"}/
    %meta{:content => "", :name => "keywords"}/
    %meta{:content => "nileforest", :name => "author"}/
    %meta{:content => "width=device-width, initial-scale=1.0, maximum-scale=1.0", :name => "viewport"}/
    /[if IE] <meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'>
    / Favicone Icon
    %link{:href => "img/favicon.ico", :rel => "shortcut icon", :type => "image/x-icon"}/
    %link{:href => "img/favicon.png", :rel => "icon", :type => "image/png"}/
    %link{:href => "img/favicon.png", :rel => "apple-touch-icon"}/
    / CSS
    %link{:href => "css/style.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "css/bootstrap.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "css/font-awesome.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "css/ionicons.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "css/plugin/jPushMenu.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "css/plugin/animate.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "css/jquery-ui.css", :rel => "stylesheet", :type => "text/css"}/
  %body.full-intro.background--dark
    / Preloader
    %section#preloader
      #loader.loader
        .loader-img
    / End Preloader
    / Site Wraper
    .wrapper
      / CONTENT -------------------------------------------------------------------------------
      / 404 Intro Section
      %section.full-screen-intro.bg-img25.overlay-dark.light-color.parallax.parallax-background2
        .content-cap-wraper
          .content-caption
            %h1.large.mb-25 404 Error
            %p.lead.content-wd650
              We're sorry but the page you Looking for can not be found.
              %br>/
              Here are some other pages.
            %br/
            %a.btn.btn-md.btn-white{:href => "home.html"}
              %i.fa.fa-angle-left.left>
              Back To Home
      .clearfix
      / End 404 Intro Section
      / End CONTENT ----------------------------------------------------------------------------
      / Scroll Top
      %a.scroll-top
        %i.fa.fa-angle-double-up
      / End Scroll Top
    / Site Wraper End
    / JS
    %script{:src => "js/jquery-1.11.2.min.js", :type => "text/javascript"}
    %script{:src => "js/jquery-ui.min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/jquery.easing.js", :type => "text/javascript"}
    %script{:src => "js/bootstrap.min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/jquery.flexslider.js", :type => "text/javascript"}
    %script{:src => "js/plugin/background-check.min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/jquery.fitvids.js", :type => "text/javascript"}
    %script{:src => "js/plugin/jquery.viewportchecker.js", :type => "text/javascript"}
    %script{:src => "js/plugin/jquery.stellar.min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/wow.min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/jquery.colorbox-min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/owl.carousel.min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/isotope.pkgd.min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/masonry.pkgd.min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/imagesloaded.pkgd.min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/jPushMenu.js", :type => "text/javascript"}
    %script{:src => "js/plugin/jquery.fs.tipper.min.js", :type => "text/javascript"}
    %script{:src => "js/plugin/mediaelement-and-player.min.js"}
    %script{:src => "js/plugin/jquery.validate.min.js", :type => "text/javascript"}
    %script{:src => "js/theme.js", :type => "text/javascript"}
    %script{:src => "js/navigation.js", :type => "text/javascript"}
