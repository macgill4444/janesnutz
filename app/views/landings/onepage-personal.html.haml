      / Contact Section
      %section#contact-section.ptb.ptb-sm-80
        .container
          .row
            .col-md-8.col-md-offset-2.text-center
              %h3 Get In Touch
          .spacer-75
          .row
            .col-md-6.col-md-offset-3
              / Contact FORM
              %form#contact.contact-form{:role => "form"}
                / IF MAIL SENT SUCCESSFULLY
                %h6.successContent
                  %i.fa.fa-check.left{:style => "color: #5cb45d;"}>
                  Your message has been sent successfully.
                / END IF MAIL SENT SUCCESSFULLY
                / MAIL SENDING UNSUCCESSFULL
                %h6.errorContent
                  %i.fa.fa-exclamation-circle.left{:style => "color: #e1534f;"}>
                  There was a problem validating the form please check!
                / END MAIL SENDING UNSUCCESSFULL
                .form-field-wrapper
                  %input#form-name.input-sm.form-full{:name => "form-name", :placeholder => "Your Name", :required => "", :type => "text"}/
                .form-field-wrapper
                  %input#form-email.input-sm.form-full{:name => "form-email", :placeholder => "Email", :required => "", :type => "email"}/
                .form-field-wrapper
                  %input#form-subject.input-sm.form-full{:name => "form-subject", :placeholder => "Subject", :type => "text"}/
                .form-field-wrapper
                  %textarea#form-message.form-full{:name => "form-message", :placeholder => "Your Message", :required => "", :rows => "7"}
                %button#form-submit.btn.btn-md.btn-black.form-full{:name => "submit", :type => "submit"} Send Message
              / END Contact FORM
